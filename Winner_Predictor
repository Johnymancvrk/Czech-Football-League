  {
   "cell_type": "code",
   "execution_count": 182,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Weekday:\n",
    "    '''\n",
    "    All odds are relevant to the beginning of Jan 2021 as the statistics are taken up to this date.\n",
    "    Only games season 2020/2021 can be analysed.\n",
    "    '''\n",
    "    \n",
    "    def __init__(self,number):\n",
    "        \n",
    "        self.number = number\n",
    "        self.r = requests.get(\"https://fbref.com/en/comps/66/schedule/Czech-First-League-Scores-and-Fixtures\")\n",
    "        self.soup = BeautifulSoup(self.r.text, \"html.parser\")\n",
    "        self.scores_table = self.soup.find('div', {'class': 'table_container'}).table\n",
    "\n",
    "        # creating empty lists for looping\n",
    "        self.gameweek = []\n",
    "        self.dayofweek = []\n",
    "        self.date = []\n",
    "        self.time = []\n",
    "        self.squad_a = []\n",
    "        self.score = []\n",
    "        self.squad_b = []\n",
    "        \n",
    "        for game in self.scores_table.find_all(\"tbody\"):\n",
    "            rows = game.find_all(\"tr\")\n",
    "            for row in rows:\n",
    "                if row.find(\"th\", {'data-stat': 'gameweek'}).text == str(self.number): #selecting only our inserted weekday\n",
    "                    self.gameweek.append(row.find(\"th\", {'data-stat': 'gameweek'}).text)\n",
    "                    self.dayofweek.append(row.find(\"td\", {'data-stat': 'dayofweek'}).text)\n",
    "                    self.date.append(row.find(\"td\", {'data-stat': 'date'}).text)\n",
    "                    self.time.append(row.find(\"td\", {'data-stat': 'time'}).text)\n",
    "                    self.squad_a.append(row.find(\"td\", {'data-stat': 'squad_a'}).text)\n",
    "                    self.score.append(row.find(\"td\", {'data-stat': 'score'}).text)\n",
    "                    self.squad_b.append(row.find(\"td\", {'data-stat': 'squad_b'}).text)\n",
    "       \n",
    "        self.bets = []\n",
    "        for idx in list(range(9)):\n",
    "            self.bets.append(round(Winner(self.squad_a[idx],self.squad_b[idx]).Odds(),2))\n",
    "            \n",
    "        self.ints = []\n",
    "        for sc in self.score:\n",
    "            try:\n",
    "                if sc[0] > sc[2]:\n",
    "                    self.ints.append([0,0.40])\n",
    "                elif sc[0] == sc[2]:\n",
    "                    self.ints.append([0.4,0.54])\n",
    "                else:\n",
    "                    self.ints.append([0.54,0.8])\n",
    "            except:\n",
    "                self.ints.append('')\n",
    "        self.conclusion = []\n",
    "        for idx,od in enumerate(self.bets):\n",
    "            try:\n",
    "                if  self.ints[idx][0] <= od <= self.ints[idx][1]:\n",
    "                    self.conclusion.append('We were successful')\n",
    "                else:\n",
    "                    self.conclusion.append('No')\n",
    "            except:\n",
    "                self.conclusion.append('Game has not been played yet!')\n",
    "        \n",
    "        self.scores = pd.DataFrame({\"Game Week\": self.gameweek, \"Weekday\": self.dayofweek, \"Date\": self.date, \"Time\": self.time, \"Home Team\": self.squad_a, \"Away Team\": self.squad_b, \"Score\": self.score,'Odds': self.bets,'Conclusion': self.conclusion})\n",
    "        \n",
    "        \n",
    "    def Table(self):        \n",
    "        return self.scores.iloc[:,1:]\n",
    "            "
   ]
  },
  {
