    "winners = []\n",
    "for idx in range(0,Scores.shape[0]):\n",
    "    if Scores.iloc[idx,:]['Home Team Goals'] > Scores.iloc[idx,:]['Away Team Goals']:\n",
    "        winners.append(int(-1))\n",
    "    elif Scores.iloc[idx,:]['Home Team Goals'] == Scores.iloc[idx,:]['Away Team Goals']:\n",
    "        winners.append(int(0))\n",
    "    else:\n",
    "        winners.append(int(1))\n",
    "        \n",
    "Scores['Winner'] = winners"
   ]
